<h1 class="contact">Contact and Connect</h1>
<!-- <p class="date-time"></p> -->
<p id="contact">Today is <%= Time.now %><br><br> Use Form to Contact JP Dickerson <br> 
</p>

<%= flash[:notice] %>
<%= form_tag("/static_pages/thank_you") do %>
<div class="row">
	<div class="col-md-4">
		<div class="form-group form-horizontal">
			<%= label_tag :name %>
			<%= text_field_tag :name, nil, class: "form-control", placeholder: "Enter Name" %>						
		</div> 
		<div class="form-group form-horizontal">
			<%= label_tag :email, "Email Address" %>
			<%= text_field_tag :email, nil, class: "form-control", placeholder: "Enter Email" %>
		</div>
		<div class="form-group form-horizontal">
			<%= label_tag :message %>
			<%= text_area_tag :message, nil, class: "form-control", placeholder: "Enter Message" %>
		</div>
	</div>
</div>
<%= submit_tag "submit", class: "submit", data: {confirm: "You are submitting a contact form! By confirming you agree to be contacted! Are you sure about this action?"} %>
<% end %>




